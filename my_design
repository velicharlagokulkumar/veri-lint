:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5b71fd9ac670 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5b71fd9ac800 .scope module, "test" "test" 3 1;
 .timescale 0 0;
v0x5b71fd9bf0e0_0 .var "clk", 0 0;
v0x5b71fd9bf1b0_0 .var "reset", 0 0;
v0x5b71fd9bf280_0 .net "value", 7 0, v0x5b71fd9beeb0_0;  1 drivers
S_0x5b71fd971490 .scope module, "c1" "counter" 3 9, 4 1 0, S_0x5b71fd9ac800;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 8 "out";
P_0x5b71fd971670 .param/l "WIDTH" 0 4 2, +C4<00000000000000000000000000001000>;
v0x5b71fd971780_0 .net "clk", 0 0, v0x5b71fd9bf0e0_0;  1 drivers
v0x5b71fd9beeb0_0 .var "out", 7 0;
v0x5b71fd9bef90_0 .net "reset", 0 0, v0x5b71fd9bf1b0_0;  1 drivers
E_0x5b71fd9acde0 .event posedge, v0x5b71fd971780_0;
    .scope S_0x5b71fd971490;
T_0 ;
    %wait E_0x5b71fd9acde0;
    %load/vec4 v0x5b71fd9bef90_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5b71fd9beeb0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5b71fd9beeb0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5b71fd9beeb0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b71fd9ac800;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b71fd9bf1b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b71fd9bf0e0_0, 0, 1;
    %end;
    .thread T_1, $init;
    .scope S_0x5b71fd9ac800;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5b71fd9bf0e0_0;
    %nor/r;
    %store/vec4 v0x5b71fd9bf0e0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5b71fd9ac800;
T_3 ;
    %vpi_call/w 3 16 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call/w 3 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5b71fd9ac800 {0 0 0};
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b71fd9bf1b0_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b71fd9bf1b0_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b71fd9bf1b0_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b71fd9bf1b0_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 23 "$stop" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5b71fd9ac800;
T_4 ;
    %vpi_call/w 3 28 "$monitor", "At time %t, value = %h (%0d)", $time, v0x5b71fd9bf280_0, v0x5b71fd9bf280_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
